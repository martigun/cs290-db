<h1>Workout Table</h1>


<table class="myTable">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Reps</th>
		<th>Weight</th>
		<th>Date</th>
		<th>LBs</th>
	</tr>
	{{#each dArray}}
		<tr>
			<td class="myTable">{{this.id}}</td>
			<td class="myTable">{{this.name}}</td>
			<td class="myTable">{{this.reps}}</td>
			<td class="myTable">{{this.weight}}</td>
			<td class="myTable">{{this.date}}</td>
			<td class="myTable">{{this.lbs}}</td>
		</tr>
	{{/each}}

</table>

